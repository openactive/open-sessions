{
  "name": "open-sessions",
  "version": "0.4.1",
  "description": "The webapp for open sessions",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git@imin:imin-ltd/open-sessions-webapp.git"
  },
  "author": "Oliver Beatson @ imin-ltd, based on boilerplate by Max Stoiber",
  "license": "ISC",
  "engines": {
    "node": "6.2.2",
    "npm": ">=3"
  },
  "scripts": {
    "npmcheckversion": "node ./internals/scripts/npmcheckversion.js",
    "db:migration:make": "node_modules/.bin/sequelize migration:create --config storage/migrations.config.js --migrations-path storage/migrations",
    "db:migration:run": "node_modules/.bin/sequelize db:migrate --config storage/migrations.config.js --migrations-path storage/migrations",
    "preinstall": "npm run npmcheckversion",
    "prebuild:app": "npm run build:app:clean",
    "prebuild:server": "npm run build:server:clean",
    "build:app:clean": "rimraf ./build/*",
    "build:app": "cross-env NODE_ENV=production webpack --config internals/webpack/webpack.prod.babel.js --color -p",
    "build:server:clean": "rimraf ./server/lib/*",
    "build:server": "webpack --config internals/webpack/webpack.server.babel.js --color",
    "analyze": "node ./internals/scripts/analyze.js",
    "start": "npm run start:dev",
    "start:dev": "npm run db:migration:run && npm run build:server && npm run start:server:dev",
    "start:prod": "npm run db:migration:run && npm run build:server && npm run start:server:prod",
    "start:prod-migrate": "npm run db:migration:run && npm run start:server:prod",
    "start:prod-nomigration": "npm run start:server:prod",
    "start:server:dev": "cross-env NODE_ENV=development node server",
    "start:server:prod": "cross-env NODE_ENV=production node server",
    "pagespeed": "node ./internals/scripts/pagespeed.js",
    "presetup": "npm i chalk",
    "setup": "node ./internals/scripts/setup.js",
    "clean": "shjs ./internals/scripts/clean.js",
    "generate": "plop --plopfile internals/generators/index.js",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint . --ignore-path .gitignore --ignore-pattern internals/scripts",
    "lint:js:staged": "eslint-staged",
    "lint:css": "stylelint ./app/**/*.css",
    "lint:css:staged": "stylelint-staged",
    "lint:staged": "npm run lint:js:staged && npm run lint:css:staged",
    "pretest": "npm run lint",
    "test": "cross-env NODE_ENV=test karma start internals/testing/karma.conf.js --single-run",
    "test:skiplint": "cross-env NODE_ENV=test karma start internals/testing/karma.conf.js --single-run",
    "test:watch": "npm run test -- --auto-watch --no-single-run",
    "test:firefox": "npm run test -- --browsers Firefox",
    "test:safari": "npm run test -- --browsers Safari",
    "test:ie": "npm run test -- --browsers IE",
    "coveralls": "cat ./coverage/lcov/lcov.info | coveralls",
    "postinstall": "npm run build:app:clean",
    "heroku-postbuild": "echo > .env && npm run build:app && npm run build:server",
    "push": "npm run test && git push"
  },
  "pre-commit": "lint:staged",
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ],
    "env": {
      "production": {
        "plugins": [
          "transform-react-remove-prop-types",
          "transform-react-constant-elements",
          "transform-react-inline-elements",
          "transform-decorators-legacy"
        ]
      }
    }
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb",
    "env": {
      "browser": true,
      "node": true,
      "mocha": true
    },
    "rules": {
      "max-len": 0,
      "new-cap": 0,
      "no-floating-decimal": 0,
      "no-param-reassign": 0,
      "strict": 0,
      "comma-dangle": "off",
      "import/no-unresolved": 0,
      "no-underscore-dangle": [
        "warn",
        {
          "allowAfterThis": true
        }
      ],
      "react/jsx-filename-extension": 0,
      "react/no-string-refs": 0,
      "react/require-extension": 0,
      "import/imports-first": 0,
      "import/no-extraneous-dependencies": 0,
      "import/newline-after-import": 0,
      "jsx-a11y/label-has-for": 0
    },
    "settings": {
      "import/resolver": {
        "webpack": {
          "config": "./internals/webpack/webpack.test.babel.js"
        }
      }
    }
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "color-hex-case": "upper",
      "string-quotes": "single",
      "font-family-name-quotes": "always-where-recommended",
      "selector-pseudo-class-no-unknown": [
        true,
        {
          "ignorePseudoClasses": [
            "global"
          ]
        }
      ],
      "indentation": 2,
      "number-leading-zero": "never"
    }
  },
  "dependencies": {
    "auth0": "^2.5.0",
    "babel-polyfill": "^6.7.4",
    "body-parser": "^1.15.1",
    "chalk": "^1.1.3",
    "compression": "^1.6.1",
    "cookie-parser": "^1.4.3",
    "cors": "^2.7.1",
    "cron": "^1.1.1",
    "cross-env": "^3.1.1",
    "dotenv": "^4.0.0",
    "express": "^4.13.4",
    "express-jwt": "^3.4.0",
    "express-session": "^1.13.0",
    "extend": "^3.0.0",
    "file-loader": "^0.8.5",
    "fontfaceobserver": "^1.7.1",
    "history": "^3.3.0",
    "immutable": "^3.8.1",
    "ip": "^1.1.2",
    "jquery": "^3.0.0",
    "moment": "^2.13.0",
    "moment-timezone": "^0.5.6",
    "multer": "^1.2.0",
    "node-fetch": "^1.6.3",
    "node-uuid": "^1.4.7",
    "passport": "^0.3.2",
    "passport-auth0": "^0.5.2",
    "pg": "^5.1.0",
    "pg-hstore": "^2.3.2",
    "postcss-custom-media": "^5.0.1",
    "react": "^15.4.2",
    "react-css-modules": "^3.7.6",
    "react-d3-basic": "^1.6.11",
    "react-datepicker": "^0.28.1",
    "react-dom": "^15.3.2",
    "react-google-maps": "^6.0.1",
    "react-helmet": "^3.1.0",
    "react-pure-render": "^1.0.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.3.0",
    "react-router-redux": "^4.0.4",
    "react-router-scroll": "^0.2.0",
    "redux": "^3.5.2",
    "redux-immutable": "^3.0.6",
    "redux-saga": "^0.10.0",
    "reselect": "^2.5.1",
    "s3-uploader": "^2.0.1",
    "sanitize.css": "^3.3.0",
    "sendgrid": "^4.7.0",
    "sequelize": "^3.30.1",
    "sequelize-cli": "^2.4.0",
    "styled-components": "^1.4.3",
    "transform-loader": "^0.2.3",
    "twitter": "^1.7.0",
    "webpack": "2.1.0-beta.20",
    "whatwg-fetch": "^1.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.7.7",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-constant-elements": "^6.8.0",
    "babel-plugin-transform-react-inline-elements": "^6.8.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.7",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.4.2",
    "cheerio": "^0.20.0",
    "coveralls": "^2.11.9",
    "cross-env": "^3.1.1",
    "css-loader": "^0.23.1",
    "enzyme": "^2.2.0",
    "eslint": "3.3.0",
    "eslint-config-airbnb": "10.0.1",
    "eslint-import-resolver-webpack": "0.6.0",
    "eslint-plugin-import": "1.13.0",
    "eslint-plugin-jsx-a11y": "2.1.0",
    "eslint-plugin-react": "6.0.0",
    "eventsource-polyfill": "^0.9.6",
    "expect": "^1.18.0",
    "expect-jsx": "^2.5.1",
    "exports-loader": "^0.6.3",
    "extract-text-webpack-plugin": "2.0.0-beta.3",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.16.0",
    "imports-loader": "^0.6.5",
    "isparta": "^4.0.0",
    "isparta-loader": "^2.0.0",
    "json-loader": "^0.5.4",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^1.0.1",
    "karma-coverage": "^1.0.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-mocha": "^1.0.1",
    "karma-mocha-reporter": "^2.0.3",
    "karma-safari-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "lint-staged": "^0.2.1",
    "lost": "^6.8.1",
    "mocha": "^3.1.0",
    "ngrok": "2.1.8",
    "null-loader": "^0.1.1",
    "offline-plugin": "^3.2.0",
    "plop": "^1.5.0",
    "postcss-cssnext": "^2.5.2",
    "postcss-focus": "^1.0.0",
    "postcss-loader": "^0.11.0",
    "postcss-nested": "^1.0.0",
    "postcss-partial-import": "^1.3.0",
    "postcss-reporter": "^1.3.3",
    "psi": "^2.0.3",
    "rimraf": "^2.5.2",
    "shelljs": "^0.7.0",
    "sinon": "^2.0.0-pre",
    "style-loader": "^0.13.1",
    "stylelint": "^6.3.3",
    "stylelint-config-standard": "^8.0.0",
    "url-loader": "^0.5.7",
    "webpack": "2.1.0-beta.20",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0"
  }
}
